@INPROCEEDINGS{4428702,
  author = {Hefeeda, M. and Bagheri, M.},
  title = {Wireless Sensor Networks for Early Detection of Forest Fires},
  booktitle = {Mobile Adhoc and Sensor Systems, 2007. MASS 2007. IEEE Internatonal
