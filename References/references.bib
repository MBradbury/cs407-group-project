@ARTICLE{4633359,
  author = {Bulut, A. and Koudas, N. and Meka, A. and Singh, A.K. and Srivastava,
	D.},
  title = {Optimization Techniques for Reactive Network Monitoring},
  journal = {Knowledge and Data Engineering, IEEE Transactions on},
  year = {2009},
  volume = {21},
  pages = {1343--1357},
  number = {9},
  month = {sept.},
  abstract = {We develop a framework for minimizing the communication overhead of
	monitoring global system parameters in IP networks and sensor networks.
	A global system predicate is defined as a conjunction of the local
	properties of different network elements. A typical example is to
	identify the time windows when the outbound traffic from each network
	element exceeds a predefined threshold. Our main idea is to optimize
	the scheduling of local event reporting across network elements for
	a given network traffic load and local event frequencies. The system
	architecture consists of N distributed network elements coordinated
	by a central monitoring station. Each network element monitors a
	set of local properties and the central station is responsible for
	identifying the status of global parameters registered in the system.
	We design an optimal algorithm, the partition and rank (PAR) scheme,
	when the local events are independent; whereas, when they are dependent,
	we show that the problem is NP-complete and develop two efficient
	heuristics: the PAR for dependent events (PAR-D) and adaptive (Ada)
	algorithms, which adapt well to changing network conditions, and
	outperform the current state of the art techniques in terms of communication
	cost.},
  doi = {10.1109/TKDE.2008.203},
  issn = {1041-4347},
  keywords = {IP networks;NP-complete;adaptive algorithms;central monitoring station;communication
	cost;dependent events;distributed network elements;global system
	parameter monitoring;heuristics;optimization techniques;outbound
	traffic;partition and rank scheme;reactive network monitoring;sensor
	networks;IP networks;communication complexity;optimisation;telecommunication
	traffic;wireless sensor networks;},
  location = {http://ieeexplore.ieee.org/xpl/articleDetails.jsp?tp=&arnumber=4633359}
}

